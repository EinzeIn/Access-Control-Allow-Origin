{
	"name": "Isolate Video Player",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Strips all content from a page except flash player",
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
	"default_icon": "off.png",
	"default_title": "Isolate Video Player"
	},
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"*://*/*"
		"activeTab"
	],
	"web_accessible_resources": [
		"on.png","off.png"
	]
}
