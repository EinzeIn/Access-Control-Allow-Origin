{
  "manifest_version": 2,

  "name": "Isolate Video Player",
  "description": "Strips all content from a page except flash player.",
  "version": "1.0",

  "permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": ["script.js"],
      "all_frames": true
    }
  ]
}
